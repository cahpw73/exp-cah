	<queryString>
		<![CDATA[select po,po.variation,order_date,po_delivery_date,po.po_title,sp.company,ct.email,po_status,por.id,po.id
from purchase_order po inner join p_order por on por.id=po.po_id
inner join supplier_proc sp on sp.id=por.supplier_id
inner join contact ct on ct.supplier_id = sp.id
